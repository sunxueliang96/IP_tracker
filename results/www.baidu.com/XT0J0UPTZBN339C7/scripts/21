
if(!location.hash.match(/[^a-zA-Z0-9]wd=/)){
    document.getElementById("wrapper").style.display='block';
    setTimeout(function(){
        try{
            var kw=document.getElementById("kw");
            kw.focus();
			kw.parentNode.className="bg s_ipt_wr iptfocus quickdelete-wrap";
        }catch(e){}
    },0);
}

